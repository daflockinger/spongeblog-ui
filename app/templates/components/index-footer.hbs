<footer class="page-footer grey">
          <div class="container">
            <div class="row">
              <div class="col l6 s12">
                <h5 class="white-text">Footer Content</h5>
                <p class="grey-text text-lighten-4">You can use rows and columns here to organize your footer content.</p>
              </div>
              <div class="col l4 offset-l2 s12">
                <h5 class="white-text">Links</h5>
                <ul>
                  {{#each categories as |footerLink|}}
                  {{#if footerLink.sideCategory}}
                  <li>{{#link-to "page" footerLink.name}}{{footerLink.name}}{{/link-to}}</li>
                  {{/if}}
                  {{/each}}
                </ul>
              </div>
            </div>
          </div>
          <div class="footer-copyright grey darken-1">
            <div class="container">
              <div class="row">
                  <div class=" col s12 m12 l12">Â© 2016 daflockinger. All rights reserved.
                  &nbsp;&nbsp;&nbsp;
                 {{#if settings.social_twitter_link}}
                  <a href="{{settings.social_twitter_link}}">{{fa-icon "twitter"}}</a> | {{/if}}
                  {{#if settings.social_facebook_link}}
                  <a href="{{settings.social_facebook_link}}">{{fa-icon "facebook"}}</a> | {{/if}}
                  {{#if settings.social_reddit_link}}
                  <a href="{{settings.social_reddit_link}}">{{fa-icon "reddit-alien"}}</a> | {{/if}}
                  {{#if settings.social_instagram_link}}
                  <a href="{{settings.social_instagram_link}}">{{fa-icon "instagram"}}</a> | {{/if}}
                  {{#if settings.social_googlep_link}}
                   <a href="{{settings.social_googlep_link}}">{{fa-icon "google-plus"}}</a> | {{/if}}
                   {{#if settings.social_youtube_link}}
                   <a href="{{settings.social_youtube_link}}">{{fa-icon "youtube"}}</a>{{/if}}
                </div>
              </div>
            </div>
          </div>
</footer>
